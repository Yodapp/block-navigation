{
	"name": "block-navigation",
	"version": "2.0.1",
	"description": "Block Navigation WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/block-navigation/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"dev": "webpack --mode development --watch --config ./webpack/build.config.babel.js",
		"prod": "webpack --mode production --config ./webpack/build.config.babel.js",
		"precopy": "rm -rf _release",
		"copy": "webpack --mode production --config ./webpack/copy.config.babel.js",
		"postcopy": "rm _release/_temp.js",
		"version": "webpack --mode production --config ./webpack/version.config.babel.js",
		"postversion": "rm _temp.js",
		"prebuild": "npm run type-check && npm run lint && npm run prettier && npm run version",
		"build": "npm run prod",
		"postbuild": "npm run copy",
		"build_v1_scripts": "git checkout v1.x && npm run prod && git checkout develop",
		"type-check": "tsc",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"prettier": "prettier --check 'src/**/*.{ts,tsx}'",
		"start": "npm run dev"
	},
	"dependencies": {
		"copy-text-to-clipboard": "^2.2.0",
		"react-tiny-popover": "^5.1.0",
		"react-window": "^1.8.5"
	},
	"devDependencies": {
		"@babel/core": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/register": "^7.10.4",
		"@types/lodash": "^4.14.157",
		"@types/react-window": "^1.8.2",
		"@types/wordpress__block-editor": "^2.2.8",
		"@types/wordpress__blocks": "^6.4.9",
		"@types/wordpress__components": "^9.0.0",
		"@types/wordpress__compose": "^3.4.2",
		"@types/wordpress__data": "^4.6.7",
		"@types/wordpress__edit-post": "^3.5.3",
		"@types/wordpress__element": "^2.14.1",
		"@types/wordpress__hooks": "^2.4.0",
		"@types/wordpress__i18n": "^3.11.0",
		"@types/wordpress__plugins": "^2.3.6",
		"@types/wordpress__rich-text": "^3.4.4",
		"@typescript-eslint/eslint-plugin": "^3.5.0",
		"@typescript-eslint/parser": "^3.5.0",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.0.3",
		"css-loader": "^3.6.0",
		"eslint": "^7.4.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.20.3",
		"file-loader": "^6.0.0",
		"mini-css-extract-plugin": "^0.9.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prettier": "^2.0.5",
		"string-replace-loader": "^2.3.0",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.9.6",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12"
	}
}