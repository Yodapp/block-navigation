{
	"name": "block-navigation",
	"version": "3.1.0",
	"description": "Block Navigation WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/block-navigation/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run test && npm run clean:dist && npm run clean:release",
		"build": "webpack --mode=production",
		"postbuild": "node build/replace-version.js && node build/copy-files.js",
		"build_v1_scripts": "git checkout v1.x && npm run prod && git checkout develop",
		"clean:dist": "rm -rf dist",
		"clean:release": "rm -rf _release",
		"dev": "webpack --mode=development",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"{build,src,types}/**/*.{js,ts,tsx}\" \"*.js\"",
		"test:prettier": "prettier --check \"{build,src,types}/**/*.{js,ts,tsx}\" \"*.js\"",
		"prettier": "prettier --write \"{build,src,types}/**/*.{js,ts,tsx}\" \"*.js\""
	},
	"dependencies": {
		"@wordpress/components": "^10.2.0",
		"@wordpress/compose": "^3.20.1",
		"@wordpress/data": "^4.23.1",
		"@wordpress/element": "^2.17.1",
		"@wordpress/hooks": "^2.9.0",
		"@wordpress/i18n": "^3.15.0",
		"@wordpress/plugins": "^2.21.1",
		"@wordpress/rich-text": "^3.21.1",
		"copy-text-to-clipboard": "^2.2.0",
		"react-tiny-popover": "^5.1.0",
		"react-window": "^1.8.5"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.11.5",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/register": "^7.11.5",
		"@types/lodash": "^4.14.161",
		"@types/react-dom": "^16.9.8",
		"@types/react-window": "^1.8.2",
		"@types/wordpress__block-editor": "^2.2.9",
		"@types/wordpress__blocks": "^6.4.10",
		"@types/wordpress__edit-post": "^3.5.4",
		"@types/wordpress__hooks": "^2.4.1",
		"@types/wordpress__plugins": "^2.3.7",
		"@typescript-eslint/eslint-plugin": "^4.2.0",
		"@typescript-eslint/parser": "^4.2.0",
		"@wordpress/browserslist-config": "^2.7.0",
		"babel-loader": "^8.1.0",
		"browserslist": "^4.14.5",
		"copyfiles": "^2.3.0",
		"css-loader": "^4.3.0",
		"eslint": "^7.10.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.2",
		"mini-css-extract-plugin": "^0.11.2",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"prettier": "^2.1.2",
		"replace-in-file": "^6.1.0",
		"stylus": "^0.54.8",
		"stylus-loader": "^3.0.2",
		"terser-webpack-plugin": "^4.2.2",
		"typescript": "^4.0.3",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.12"
	},
	"resolutions": {
		"nib/stylus": "0.54.8"
	}
}
