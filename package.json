{
	"name": "block-navigation",
	"version": "3.4.0",
	"description": "Block Navigation",
	"homepage": "https://wordpress.org/plugins/block-navigation",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run test:types && npm run clean:dist && npm run clean:release",
		"build": "webpack --mode=production",
		"postbuild": "node scripts/replace-version.js && node scripts/copy-files.js",
		"clean:dist": "rm -rf dist",
		"clean:release": "rm -rf _release",
		"dev": "webpack --mode=development",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"**/*.{js,ts,tsx}\"",
		"test:prettier": "prettier --check \"**/*.{js,ts,tsx,php,json}\"",
		"prettier": "prettier --write \"**/*.{js,ts,tsx,php,json}\""
	},
	"dependencies": {
		"@babel/core": "^7.14.6",
		"@babel/preset-env": "^7.14.7",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.14.5",
		"@babel/register": "^7.14.5",
		"@types/lodash": "^4.14.171",
		"@types/react-window": "^1.8.4",
		"@types/wordpress__block-editor": "^6.0.2",
		"@types/wordpress__components": "^14.0.2",
		"@types/wordpress__compose": "^4.0.0",
		"@types/wordpress__core-data": "^2.4.5",
		"@types/wordpress__data": "^4.6.10",
		"@types/wordpress__edit-post": "^4.0.1",
		"@types/wordpress__plugins": "^3.0.0",
		"@types/wordpress__rich-text": "^3.4.6",
		"@wordpress/browserslist-config": "^4.0.1",
		"@wordpress/element": "^3.1.1",
		"@wordpress/hooks": "^3.1.1",
		"@wordpress/i18n": "^4.1.1",
		"babel-loader": "^8.2.2",
		"browserslist": "^4.16.6",
		"copy-text-to-clipboard": "^3.0.1",
		"copyfiles": "^2.4.1",
		"css-loader": "^5.2.6",
		"css-minimizer-webpack-plugin": "^3.0.2",
		"mini-css-extract-plugin": "^2.1.0",
		"nib": "^1.1.2",
		"react-tiny-popover": "^6.0.5",
		"replace-in-file": "^6.2.0",
		"stylus": "^0.54.8",
		"stylus-loader": "^6.1.0",
		"terser-webpack-plugin": "^5.1.4",
		"typescript": "^4.3.5",
		"webpack": "^5.44.0",
		"webpack-cli": "^4.7.2"
	},
	"devDependencies": {
		"@prettier/plugin-php": "^0.17.2",
		"@typescript-eslint/eslint-plugin": "^4.28.2",
		"@typescript-eslint/parser": "^4.28.2",
		"eslint": "^7.30.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"prettier": "^2.3.2"
	}
}
