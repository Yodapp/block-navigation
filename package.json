{
	"name": "block-navigation",
	"version": "3.0.0",
	"description": "Block Navigation WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/block-navigation/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"dev": "webpack --mode=development",
		"prebuild": "rm -rf dist && rm -rf _release && npm run type-check && npm run prettier && npm run lint",
		"build": "webpack --mode=production",
		"postbuild": "npm run build_v1_scripts && node build/replace-version.js && node build/copy-files.js",
		"build_v1_scripts": "git checkout v1.x && npm run prod && git checkout develop",
		"type-check": "tsc",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"prettier": "prettier --write 'src/**/*.{ts,tsx}'"
	},
	"dependencies": {
		"@wordpress/components": "^10.0.3",
		"@wordpress/compose": "^3.19.3",
		"@wordpress/data": "^4.22.3",
		"@wordpress/element": "^2.16.0",
		"@wordpress/hooks": "^2.9.0",
		"@wordpress/i18n": "^3.14.0",
		"@wordpress/plugins": "^2.20.3",
		"@wordpress/rich-text": "^3.20.3",
		"copy-text-to-clipboard": "^2.2.0",
		"react-tiny-popover": "^5.1.0",
		"react-window": "^1.8.5"
	},
	"devDependencies": {
		"@babel/core": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/register": "^7.10.5",
		"@types/lodash": "^4.14.158",
		"@types/react-dom": "^16.9.8",
		"@types/react-window": "^1.8.2",
		"@types/wordpress__block-editor": "^2.2.8",
		"@types/wordpress__blocks": "^6.4.9",
		"@types/wordpress__edit-post": "^3.5.3",
		"@types/wordpress__hooks": "^2.4.0",
		"@types/wordpress__plugins": "^2.3.6",
		"@typescript-eslint/eslint-plugin": "^3.7.1",
		"@typescript-eslint/parser": "^3.7.1",
		"@wordpress/browserslist-config": "^2.7.0",
		"babel-loader": "^8.1.0",
		"browserslist": "^4.13.0",
		"copyfiles": "^2.3.0",
		"css-loader": "^4.2.0",
		"eslint": "^7.6.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.20.5",
		"mini-css-extract-plugin": "^0.9.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prettier": "^2.0.5",
		"replace-in-file": "^6.1.0",
		"stylus": "^0.54.8",
		"stylus-loader": "^3.0.2",
		"terser-webpack-plugin": "^3.0.8",
		"typescript": "^3.9.7",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	}
}