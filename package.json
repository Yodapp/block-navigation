{
	"name": "block-navigation",
	"version": "2.0.1",
	"description": "Block Navigation WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/block-navigation/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"dev": "webpack --mode=development",
		"prebuild": "rm -rf build && rm -rf _release && npm run type-check && npm run lint",
		"build": "webpack --mode=production",
		"postbuild": "npm run build_v1_scripts && node build-replace-version.js && node build-copy-files.js",
		"build_v1_scripts": "git checkout v1.x && npm run prod && git checkout develop",
		"type-check": "tsc",
		"lint": "eslint 'src/**/*.{ts,tsx}'"
	},
	"dependencies": {
		"copy-text-to-clipboard": "^2.2.0",
		"react-tiny-popover": "^5.1.0",
		"react-window": "^1.8.5"
	},
	"devDependencies": {
		"@babel/core": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/register": "^7.10.4",
		"@types/lodash": "^4.14.157",
		"@types/react-window": "^1.8.2",
		"@types/wordpress__block-editor": "^2.2.8",
		"@types/wordpress__blocks": "^6.4.9",
		"@types/wordpress__components": "^9.0.0",
		"@types/wordpress__compose": "^3.4.2",
		"@types/wordpress__data": "^4.6.7",
		"@types/wordpress__edit-post": "^3.5.3",
