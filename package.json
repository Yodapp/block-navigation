{
	"name": "block-navigation",
	"version": "3.1.0",
	"description": "Block Navigation",
	"homepage": "https://wordpress.org/plugins/block-navigation",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run test && npm run clean:dist && npm run clean:release",
		"build": "webpack --mode=production",
		"postbuild": "node scripts/replace-version.js && node scripts/copy-files.js",
		"build_v1_scripts": "git checkout v1.x && npm run prod && git checkout develop",
		"clean:dist": "rm -rf dist",
		"clean:release": "rm -rf _release",
		"dev": "webpack --mode=development",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"{build,src,types}/**/*.{js,ts,tsx}\" \"*.js\"",
		"test:prettier": "prettier --check \"{build,src,types}/**/*.{js,ts,tsx}\" \"*.{js,php}\"",
		"prettier": "prettier --write \"{build,src,types}/**/*.{js,ts,tsx}\" \"*.{js,php}\""
	},
	"dependencies": {
		"@wordpress/components": "^11.1.1",
		"@wordpress/compose": "^3.22.0",
		"@wordpress/data": "^4.25.0",
		"@wordpress/element": "^2.18.0",
		"@wordpress/hooks": "^2.10.0",
		"@wordpress/i18n": "^3.16.0",
		"@wordpress/plugins": "^2.23.0",
		"@wordpress/rich-text": "^3.23.0",
		"copy-text-to-clipboard": "^2.2.0",
		"react-tiny-popover": "^5.1.0",
		"react-window": "^1.8.6"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.5",
		"@babel/preset-typescript": "^7.12.1",
		"@babel/register": "^7.12.1",
		"@prettier/plugin-php": "^0.16.0",
		"@types/lodash": "^4.14.165",
		"@types/react-dom": "^16.9.9",
		"@types/react-window": "^1.8.2",
		"@types/wordpress__block-editor": "^2.2.9",
		"@types/wordpress__blocks": "^6.4.12",
		"@types/wordpress__edit-post": "^3.5.4",
		"@types/wordpress__hooks": "^2.4.1",
		"@types/wordpress__plugins": "^2.3.7",
		"@typescript-eslint/eslint-plugin": "^4.7.0",
		"@typescript-eslint/parser": "^4.7.0",
		"@wordpress/browserslist-config": "^2.7.0",
		"babel-loader": "^8.2.1",
		"browserslist": "^4.14.7",
		"copyfiles": "^2.4.0",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^1.1.5",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.5",
		"mini-css-extract-plugin": "^1.3.1",
		"nib": "^1.1.2",
		"prettier": "^2.1.2",
		"replace-in-file": "^6.1.0",
		"stylus": "^0.54.8",
		"stylus-loader": "^4.3.0",
		"terser-webpack-plugin": "^5.0.3",
		"typescript": "^4.0.5",
		"webpack": "^5.4.0",
		"webpack-cli": "^4.2.0"
	}
}
